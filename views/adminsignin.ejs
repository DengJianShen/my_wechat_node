<%- include('./components/head') %>
<div class="container">
  <div class="row">
    <div class="page-header">
      <h1><%= title %></h1>
    </div>
    <form action="/admin/dosignin" method="POST">
      <div class="modal-header">登录</div>
      <div class="modal-body">
        <div class="form-group">
          <label for="adminname">用户名:<input type="text" class="form-control" type="text" name="adminname"/></label>
        </div>
        <div class="form-group">
          <label for="adminpassword">密码:<input type="password" class="form-control" type="text" name="adminpassword"/></label>
        </div>
      </div>
      <div class="modal-footer">
        <button type="submit" class="btn btn-success">登录</button>
        <button type="button" class="btn btn-default" onclick="location='/admin/signup'">注册</button>
      </div>
    </form>
  </div>
</div>
  </body>
</html>